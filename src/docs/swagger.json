    "components": {
        "responses": {
            "Unauthorized": {
                "description": "Não autorizado",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "field": {
                                        "type": "string",
                                        "example": "token"
                                    },
                                    "message": {
                                        "type": "string",
                                        "example": "Usuário não autenticado!"
                                    },
                                    "statusCode": {
                                        "type": "integer",
                                        "example": 401
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
