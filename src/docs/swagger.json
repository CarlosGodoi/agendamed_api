            "Authenticate": {
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "description": "Email do usuário"
                    },
                    "password": {
                        "type": "string",
                        "format": "password",
                        "description": "Senha do usuário"
                    }
                }
            },
