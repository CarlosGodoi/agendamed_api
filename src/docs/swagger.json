                    },
                    "crm": {
                        "type": "string",
                        "description": "CRM do médico"
                    },
                    "specialtyName": {
                        "type": "string",
                        "description": "Especialidade médica"
                    }
                }
            },
            "Doctors": {
                "type": "object",
                "required": [],
                "properties": {}
