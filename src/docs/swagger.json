                    },
                    "name": {
                        "type": "string",
                        "description": "Nome do usuário"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "description": "Email do usuário"
                    },
                    "cpf": {
                        "type": "string",
                        "description": "CPF do usuário"
                    },
                    "role": {
                        "type": "string",
                        "description": "Papel do usuário no sistema"
                    },
                    "password": {
                        "type": "string",
                        "format": "password",
                        "description": "Senha do usuário"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Data de criação"
                    }
